<html>
  <head>
    <title>OSC Tool</title>
    <link rel="stylesheet" type="text/css" href="css/style.css"/>
    <script src="bundle.js"></script>
    <script>
document.addEventListener("DOMContentLoaded", function(event) {
  var data = <%- jsonData %>;
  var hostUrl = "<%= hostUrl %>";
  oscQuery.initWebSocket(hostUrl.replace("http", "ws"));
  oscQuery.buildFromQueryResult(data);
  oscQuery.addInputEventHandlers();
});
    </script>
  </head>
  <body>
    <div id="mainContents"></div>
  </body>
</html>
